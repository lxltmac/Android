<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="lin.linorder.models.goods.FoodStyle" table="foodstyle">
		<!-- 配置主键 -->
		<id name="id">
			<generator class="native" />
		</id>
		<!-- 配置其他属性 -->
		<property name="foodStyleId" />
		<property name="styleName" />
		<!-- 配置外键 -->
		<list name="foods" table="food" inverse="true" cascade="save-update">
			<key column="foodStyleId" not-null="true" />
			<list-index column="id" />
			<one-to-many class="lin.linorder.models.goods.Food"/>
		</list>
	</class>
</hibernate-mapping>