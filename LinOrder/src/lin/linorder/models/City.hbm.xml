<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="lin.linorder.models.City" table="city">
		<!-- 配置主键 -->
		<id name="cityID">
			<generator class="native" />
		</id>
		<!-- 配置其他属性 -->
		<property name="city" />
		<!-- 配置外键 -->
		<many-to-one name="province" class="lin.linorder.models.Province" column="provinceID" not-null="true" cascade="save-update" fetch="select" lazy="false" />
	</class>
</hibernate-mapping>